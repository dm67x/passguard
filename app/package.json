{
  "name": "app",
  "productName": "Passguard",
  "version": "1.0.0",
  "description": "Password manager",
  "main": "build/index.js",
  "scripts": {
    "start": "concurrently \"babel --watch src --out-dir build --source-maps inline --copy-files\" \"electron-forge start\"",
    "package": "babel src --out-dir build --copy-files && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "dm67x",
    "email": "dm67x@outlook.fr"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": []
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "electron-squirrel-startup": "^1.0.0",
    "ffi-napi": "^4.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "ref-napi": "^3.0.2",
    "ref-struct-di": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.54",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-transform-import-css": "^1.0.0-alpha.11",
    "concurrently": "^6.0.0",
    "electron": "12.0.1"
  }
}